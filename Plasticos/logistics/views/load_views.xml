<odoo>
    <record id="view_load_tree" model="ir.ui.view">
        <field name="name">linda.load.tree</field>
        <field name="model">linda.load</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="carrier_id"/>
